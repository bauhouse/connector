events.each do |event|
  xm.item do
    xm.title event.name
    xm.description render(:partial => "event", :locals => { :event => event })
    xm.pubDate feed_date(event)
    xm.guid calendar_link_for(event)
    xm.link calendar_link_for(event)
  end
end