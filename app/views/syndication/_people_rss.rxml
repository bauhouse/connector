people.each do |person|
  xm.item do
    xm.title person.full_name
    xm.description render(:partial => "person", :locals=>{ :person => person })
    xm.pubDate feed_date(person)
    xm.guid person_show_url(:id => person.id, :only_path => false)
  end
end